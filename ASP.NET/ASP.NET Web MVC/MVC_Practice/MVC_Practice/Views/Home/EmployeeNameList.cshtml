
@model IEnumerable<MVC_Practice.Models.Employee>

@using MVC_Practice.Models;

<div style="font-family:Arial">
    @{
        ViewBag.Title = "Employee List";
    }

    <h2>Employee List</h2>
    <ul>
        @foreach (Employee employee in @Model)
        {
            <li>@Html.ActionLink(employee.Name, "GetEmployeeDetailsByID", new { id = employee.EmployeeId })</li>
        }
    </ul>
</div>